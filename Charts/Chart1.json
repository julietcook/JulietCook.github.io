
{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  
  "title": {
    "text": "GDP vs Women's Labour Force Participation Rate",
    "subtitle":"Interactive Scatterplot, source: WorldBank ",
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "start",
    "color": "black"
  },
  
   "data": {
