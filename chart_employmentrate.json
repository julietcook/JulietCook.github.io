{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
      "text": "Employment rate in % ",
      "subtitle":"Data scrapped form Wikipedia",
      "subtitleFontStyle":"italic",
      "subtitleFontSize":10,
      "anchor": "start",
      "color": "black"
    },
    
     "data": {
      "url": "https://raw.githubusercontent.com/julietcook/JulietCook.github.io/main/Datasets/week7/countries-employmentrate.csv",
      "format": {"type": "csv"}
    },
  
    "height": 300,
    "width": 300,


  "mark": {"type":"bar","color":"#228C4E", "tooltip": true},
  
  "encoding": {
    "x": {"field": "Country", "type": "nominal", "axis": {"labelAngle":30},"title": ""},
    "y": {"field": "Employment Rate", "type": "quantitative","title": "Employment rate (%)"}
  }



}
